<h1>Listing languages</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Designed by</th>
    </tr>
  </thead>

  <tbody>
    <% @languages.each do |language| %>
      <tr>
        <td><%= language.name %></td>
        <td></td>
        <td></td>
      </tr>
    <% end %>
  </tbody>
</table>
